<org.distributedea.ontology.job.noontology.Job>
  <jobID>onlyInit</jobID>
  <numberOfRuns>1</numberOfRuns>
  <description>Only Initialization</description>
  <problemToSolve>org.distributedea.ontology.problem.ProblemContinousOpt</problemToSolve>
  <problemFileName>inputs/f01.co</problemFileName>
  <methodsFileName>configuration/methodsAll.txt</methodsFileName>
  <problemTools>
    <problemTools>
      <java-class>org.distributedea.problems.continuousoptimization.ProblemToolRandomMove</java-class>
    </problemTools>
  </problemTools>
  <individualDistribution>true</individualDistribution>
  <planners>
    <org.distributedea.agents.systemagents.centralmanager.planner.initialisation.PlannerInitialisation>
      <state>RUN_ONE_AGENT_PER_CORE</state>
      <methodRepetition>true</methodRepetition>
    </org.distributedea.agents.systemagents.centralmanager.planner.initialisation.PlannerInitialisation>
  </planners>
  <plannerTypes>
    <org.distributedea.agents.systemagents.centralmanager.plannertype.PlannerTypeTimeRestriction>
      <countOfReplaning>50</countOfReplaning>
      <iterationNumI>0</iterationNumI>
    </org.distributedea.agents.systemagents.centralmanager.plannertype.PlannerTypeTimeRestriction>
  </plannerTypes>
</org.distributedea.ontology.job.noontology.Job>
